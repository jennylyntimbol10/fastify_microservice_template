            "port": "APP_CONFIG_PORT",
            "jwt": {
                "enable_validation": "APP_CONFIG_JWT_ENABLE_VALIDATION",
                "auth": {
                    "publickey": "APP_CONFIG_JWT_AUTH_PUBLICKEY"
                }
            }
        },
        "logging": {
            "log_file_location": "APP_LOGGING_LOG_FILE_LOCATION",
            "log_file_name": "APP_LOGGING_LOG_FILE_NAME",
