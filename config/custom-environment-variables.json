            "log_timestamp_format": "APP_LOGGING_LOG_TIMESTAMP_FORMAT",
            "log_file_date_pattern": "APP_LOGGING_LOG_FILE_DATE_PATTERN",
            "log_file_max_size": "APP_LOGGING_LOG_FILE_MAX_SIZE",
